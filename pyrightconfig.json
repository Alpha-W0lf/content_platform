{
  "include": ["src"],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "**/venv",
    "**/.venv",
    "**/site-packages",
    "**/.pytest_cache",
    "**/.mypy_cache"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.12",
  "typeCheckingMode": "basic",
  "strictParameterNoneValue": false,
  "reportUnknownParameterType": "none",
  "reportMissingParameterType": "none",
  "useLibraryCodeForTypes": true
}